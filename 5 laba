using System;

namespace _5th_Lab
{
    class Program
    {
        static int factorial(int x)
        {
            int f = 1;
            for (int i = 1; i < x; i++)
            {
                f *= i;
            }
            return f;
            
        }
        static int var(int k, int n) 
        {
            return factorial(n)/(factorial(k)*factorial(n-k));
        }

    }
}
